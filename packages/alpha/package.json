{
  "name": "@finsweet/alpha",
  "version": "1.0.2",
  "description": "Developer starter template for Finsweet projects.",
  "homepage": "https://github.com/finsweet/alpha#readme",
  "license": "ISC",
  "keywords": [],
  "author": {
    "name": "Finsweet",
    "url": "https://www.finsweet.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/finsweet/alpha.git"
  },
  "bugs": {
    "url": "https://github.com/finsweet/alpha/issues"
  },
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development node ./bin/build.js",
    "build": "cross-env NODE_ENV=production node ./bin/build.js",
    "lint": "eslint --ignore-path .gitignore ./src && prettier --check ./src",
    "check": "tsc --noEmit",
    "format": "prettier --write ./src",
    "test": "pnpm playwright test",
    "test:headed": "pnpm playwright test --headed",
    "update": "pnpm update -i -L"
  },
  "devDependencies": {
    "@finsweet/playwright-config": "workspace:*"
  },
  "dependencies": {
    "@finsweet/ts-utils": "^0.33.1"
  }
}
